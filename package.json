{
    "name": "pinpoint-web",
    "version": "0.0.1",
    "description": "pinpoint-web",
    "repository": {
        "type": "github",
        "url": "https://github.com/ld000/pinpoint-web.git"
    },
    "scripts": {
        "start": "webpack-dev-server --watch --history-api-fallback --hot --inline --process --colors --port 3000",
        "build": "webpack --config webpack-dist.config.js",
        "test": "PWD=$(pwd) ava --verbose",
        "lint": "eslint ./ --cache --ignore-pattern .gitignore",
        "precommit-msg": "echo Pre-commit checking...please wait && exit 0",
        "dll": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
        "dev-dll": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
        "clean": "rimraf ./dist",
        "deploy": "npm run clean && npm run dll && npm run build && npm run upload"
    },
    "author": "lidong9144@163.com",
    "devDependencies": {
        "ali-oss": "^4.6.2",
        "autoprefixer": "^6.7.2",
        "ava": "~0.15.2",
        "babel": "~6.5.2",
        "babel-cli": "~6.11.4",
        "babel-core": "~6.10.4",
        "babel-eslint": "~6.1.2",
        "babel-loader": "~6.2.4",
        "babel-plugin-espower": "~2.3.1",
        "babel-plugin-import": "^1.0.0",
        "babel-plugin-transform-decorators-legacy": "~1.3.4",
        "babel-plugin-transform-runtime": "~6.9.0",
        "babel-plugin-webpack-alias": "~2.1.1",
        "babel-preset-es2015": "~6.9.0",
        "babel-preset-react": "~6.11.1",
        "babel-preset-stage-0": "~6.5.0",
        "babel-register": "~6.9.0",
        "cross-env": "^3.1.4",
        "css-loader": "~0.23.1",
        "enzyme": "^2.8.2",
        "eslint": "~3.3.1",
        "eslint-config-airbnb": "~10.0.1",
        "eslint-plugin-babel": "~3.3.0",
        "eslint-plugin-import": "~1.13.0",
        "eslint-plugin-jsx-a11y": "~2.1.0",
        "eslint-plugin-react": "~6.1.1",
        "extract-text-webpack-plugin": "~1.0.1",
        "file-loader": "~0.9.0",
        "ghooks": "^1.3.2",
        "happypack": "^2.2.1",
        "html-webpack-plugin": "~2.22.0",
        "http-proxy": "~1.14.0",
        "jsdom": "~9.4.1",
        "less": "~2.7.1",
        "less-loader": "~2.2.3",
        "node-sass": "^4.5.0",
        "nyc": "~7.0.0",
        "open-browser-webpack-plugin": "0.0.3",
        "postcss-loader": "~0.9.1",
        "prepare-commit-msg-dianwoda": "^0.2.1",
        "react-addons-test-utils": "~15.2.1",
        "react-hot-loader": "~1.3.0",
        "redux-ava": "~2.1.0",
        "rimraf": "^2.5.4",
        "sass-loader": "^6.0.0",
        "sinon": "~1.17.4",
        "style-loader": "~0.13.1",
        "url-loader": "~0.5.7",
        "validate-commit-msg": "^2.8.0",
        "webpack": "~1.13.1",
        "webpack-dev-middleware": "^1.8.4",
        "webpack-dev-server": "~1.14.1",
        "webpack-hot-middleware": "^2.13.2"
    },
    "dependencies": {
        "ali-cdn-sdk": "^1.2.0",
        "antd": "^2.8.0",
        "babel-polyfill": "~6.9.1",
        "babel-runtime": "~6.9.2",
        "classnames": "^2.2.5",
        "draft-js": "^0.9.1",
        "echarts": "~3.2.3",
        "immutable": "~3.8.1",
        "isomorphic-fetch": "~2.2.1",
        "moment": "~2.14.1",
        "rc-queue-anim": "^0.13.0",
        "rc-tween-one": "~0.8.5",
        "react": "~15.3.2",
        "react-copy-to-clipboard": "~4.2.3",
        "react-dom": "~15.2.1",
        "react-images": "~0.4.11",
        "react-redux": "~4.4.5",
        "react-router": "~2.6.0",
        "react-router-redux": "~4.0.5",
        "redux": "~3.5.2",
        "redux-actions": "~0.10.1",
        "redux-thunk": "~2.1.0"
    },
    "ava": {
        "files": [
            "./app/**/*.test.js"
        ],
        "require": [
            "ignore-styles",
            "./test/setup-test-env.js"
        ],
        "source": [
            "**/*.{js,jsx}",
            "!dist/**/*"
        ],
        "failFast": true,
        "tap": false,
        "babel": "inherit"
    }
}